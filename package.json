  "scripts": {
    "dev": "next dev",
    "dev:withsockets": "concurrently \"next dev\" \"npm run workers:alt\"",
    "build": "next build && npm run build:worker",
    "build:worker": "tsc -p tsconfig.worker.json",
    "build:strict": "tsc --noEmit && next build",
    "start": "next start",
    "start:withsockets": "concurrently \"next start\" \"npm run workers\"",
    "workers": "ts-node -r tsconfig-paths/register worker.ts",
    "workers:dev": "ts-node-dev -r tsconfig-paths/register worker.ts",
    "workers:alt": "ts-node-dev -r tsconfig-paths/register --project tsconfig.worker.json --transpile-only --respawn worker.ts"
